{
  "name": "blog-rest",
  "version": "1.0.0",
  "description": "A rest server for Coleman's blog site",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "set NODE_ENV=dev&& ts-node-dev v1/server.ts",
    "netlify-dev": "netlify dev",
    "netlify-prod": "netlify dev --context production",
    "test": "set NODE_ENV=test&& jest --watchAll --detectOpenHandles --config=./v1/jest.config.ts"
  },
  "author": "Coleman Sperando",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "axios": "^1.3.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "netlify": "^13.1.4",
    "path": "^0.12.7",
    "serverless-http": "^3.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-hooks": "^1.1.2",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "netlify-cli": "^2.12.0"
  }
}
