{
  "name": "blog-rest",
  "version": "1.0.0",
  "description": "A rest server for Coleman's blog site",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "set NODE_ENV=dev&& ts-node-dev v1/server.ts",
    "test": "set NODE_ENV=test&& jest --detectOpenHandles --config=./v1/jest.config.ts --bail --forceExit",
    "test-local": "set NODE_ENV=test&& jest --watchAll --detectOpenHandles --config=./v1/jest.config.ts --coverage",
    "start": "node dist/server.js"
  },
  "author": "Coleman Sperando",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.15.3",
    "@types/winston": "^2.4.4",
    "axios": "^1.3.6",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-hooks": "^1.1.2",
    "typescript": "^5.0.2",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  }
}
